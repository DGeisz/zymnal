{
  "name": "zymnal",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:react2.helloWorld"
  ],
  "main": "./ex_out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "react2.helloWorld",
        "title": "Hello World"
      }
    ],
    "customEditors": [
      {
        "viewType": "zym.zymFileEditor",
        "displayName": "Zym",
        "selector": [
          {
            "filenamePattern": "*.zym"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "allotment": "^1.17.0",
    "axios": "^0.27.2",
    "clsx": "^1.2.1",
    "darkreader": "^4.9.55",
    "escape-latex": "^1.2.0",
    "eventemitter3": "^4.0.7",
    "katex": "^0.15.6",
    "react": "^18.1.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.1.0",
    "react-icons": "^4.4.0",
    "react-modal": "^3.16.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "underscore": "^1.13.4",
    "web-vitals": "^2.1.0",
    "zod": "^3.17.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p tsconfig-extension.json",
    "test": "react-app-rewired test",
    "server": "cd src/zym_persistence_server && yarn start",
    "server:dev": "cd src/zym_persistence_server && yarn dev",
    "test:debug": "react-app-rewired --inspect-brk test --runInBand --no-cache",
    "eject": "react-app-rewired eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/escape-latex": "^0.1.30",
    "@types/glob": "^8.0.0",
    "@types/jest": "^27.0.1",
    "@types/katex": "^0.14.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-icons": "^3.0.0",
    "@types/react-modal": "^3.13.1",
    "@types/underscore": "^1.11.4",
    "@types/vscode": "^1.72.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vscode/test-electron": "^2.1.5",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.0.24"
  }
}
